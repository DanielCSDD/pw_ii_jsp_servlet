<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>pw_ii_jsp_servlet</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>jakarta.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
    <url-pattern>/faces/*</url-pattern>
  </servlet-mapping>
  
  <!-- Mapeando a servlet da aplicação -->
  <servlet>
		<description></description>
		<servlet-name>ContatoServlet</servlet-name>
		<servlet-class>br.com.projeto.controller.ContatoServlet</servlet-class>
	</servlet>
	
  <servlet-mapping>
  	<servlet-name>ContatoServlet</servlet-name>
  	<url-pattern>/viewagenda</url-pattern>
  	<url-pattern>/viewnovo</url-pattern>
  	<url-pattern>/inserir</url-pattern>
  	<url-pattern>/vieweditar</url-pattern>
  	<url-pattern>/atualizar</url-pattern>
  	<url-pattern>/excluir</url-pattern>
  	<url-pattern>/impressao</url-pattern>
  </servlet-mapping>
</web-app>